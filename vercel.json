{
  "version": 2,
  "buildCommand": "cd apps/web-editor && npm install && npm run build",
  "outputDirectory": "apps/web-editor/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "apps/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}

